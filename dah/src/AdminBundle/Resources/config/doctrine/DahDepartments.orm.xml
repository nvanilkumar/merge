<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdminBundle\Entity\DahDepartments" table="dah_departments">
    <id name="deptid" type="integer" column="deptid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="department" type="string" column="department" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="status" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addedOn" type="integer" column="added_on" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="updatedOn" type="integer" column="updated_on" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
