{
    "manifest_version": 2,
   
     "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://*.googleapis.com; object-src 'self'",
 
    "name": "Bookmark Extension Example",
    "description": "POST details of the current page to a remote endpoint.",
    "version": "0.2",
    "background": {
        "scripts": ["event.js","background.js"], 
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
            "notifications",
        "tabs", 
        "activeTab",
         "activeTab",
        "http://*/*", 
        "https://*/*"
    ]
 

   


}
